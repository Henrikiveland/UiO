<?xml version="1.0" ?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <xsd:element name="audit">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="subject" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="code" type="xsd:string"/>
                            <xsd:element name="title" type="xsd:string"/>
                            <xsd:element name="statistics" minOccurs="0" maxOccurs="unbounded">
                                <xsd:complexType>
                                    <xsd:element name="enrol" type="xsd:integer"/>
                                    <xsd:element name="withdrawn" type="xsd:integer"/>
                                </xsd:complexType>
                            <xsd:element/>
                        </xsd:sequence>
                        <xsd:attribute name="sid" type="xsd:integer"/>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
            <xsd:attribute name="campus" type="xsd:string"/>
            <xsd:attribute name="year" type="xsd:integer"/>
            <xsd:attribute name="session" type="xsd:string"/>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>