<?xml version="1.0" ?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <xsd:element name="result">
        <xsd:complexType>
            <xsd:sequence>
            <xsd:element name="contestantId" type="xsd:string"/>
            <xsd:element name="examId" type="xsd:string"/>
            <xsd:element name="score" type="xsd:integer"/>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:INTEGER">
                        <xsd:minInclusive value="0"/>
                        <xsd:maxInclusive value="300"/>
                    </xsd:restriction>
                </xsd:simpleType>

            <xsd:element name="band" type="xsd:string"/>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="A"/>
                        <xsd:enumeration value="B"/>
                        <xsd:enumeration value="C"/>
                        <xsd:enumeration value="D"/>
                    </xsd:restriction>
                </xsd:simpleType>
            <xsd:element name="digitalSignature" type="xsd:string"/>
            </xsd:sequence>
            <xsd:attribute name="ref" type="xsd:string"/>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>